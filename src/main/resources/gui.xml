<?xml version="1.0" encoding="UTF-8" ?>
<Frame
        width="600"
        height="500"
        title="App"
        onclose="exit"
>

    <GridBag is-content-pane="" row-weights="0 1" rows="0 1" column-weights="0.2 ">
        <GridBag  column-weights="0 1" columns="0 1" insets="15" rows="0 0 0 0 0 0"
                  row-weights="0 0 0 0 0 0">
                <Label row="0" col="0" font-style="bold">Nombre</Label>
                <TextField row="0" col="1" fill="horizontal" id="nombre" cols="10" padding="5" form="obj" required=""/>
                <Label row="1" column="0" font-style="bold">Año</Label>
                <TextField row="1" col="1" fill="horizontal" id="anio" form="obj" required=""/>
                <Label row="2" column="0" font-style="bold">Slogan</Label>
                <TextField row="2" col="1" fill="horizontal" id="slogan" form="obj" required=""/>
                <Label row="3" column="0" font-style="bold">Url</Label>
                <TextField row="3" col="1" fill="horizontal" id="url" form="obj" required=""/>
                <Label row="4" col="0" font-style="bold">Ubicación</Label>
                <TextField row="4" col="1" fill="horizontal" id="ubicacion" name="ubicación" form="obj" required=""/>
                <Flow row="5" col="1" fill="horizontal" layout="right">
                    <Button id="guardar">Guardar</Button>
                    <Button id="cancelar">Cancelar</Button>
                </Flow>
            </GridBag>
            <ScrollPane row="1" fill="both">
                <Table>
                    <th>
                        <td>Nombre</td>
                        <td>Año</td>
                        <td>Ubicación</td>
                        <td>Slogan</td>
                        <td>Url</td>
                    </th>
                    <tr>
                        <td>Nombre</td>
                        <td>Año</td>
                        <td>Ubicación</td>
                        <td>Slogan</td>
                        <td>Url</td>
                    </tr>
                </Table>
            </ScrollPane>
        </GridBag>

</Frame>